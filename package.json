{
  "name": "@opendevstack/ods-selector-widget",
  "version": "0.0.1",
  "description": "A flexible and customizable web component for displaying platform navigation and resource links",
  "author": "OpenDevStack",
  "repository": {
    "type": "git",
    "url": "https://github.com/opendevstack/ods-selector-widget.git"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/ods-selector-widget/ods-selector-widget.esm.js",
  "exports": {
    ".": {
      "import": "./dist/ods-selector-widget/ods-selector-widget.esm.js",
      "require": "./dist/ods-selector-widget/ods-selector-widget.cjs.js"
    },
    "./ods-selector-widget": {
      "import": "./dist/components/ods-selector-widget.js",
      "types": "./dist/components/ods-selector-widget.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e --coverage",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^4.27.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^24.3.0"
  },
  "license": "Apache-2.0"
}
